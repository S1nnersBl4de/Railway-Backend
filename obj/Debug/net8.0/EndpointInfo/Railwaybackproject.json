              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/BookingRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Booking/my-bookings": {
      "get": {
        "tags": [
          "Booking"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Booking/all-bookings": {
      "get": {
        "tags": [
          "Booking"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Train": {
      "post": {
        "tags": [
          "Train"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TrainRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TrainRequest"
              }
            },
            "application/*+json": {
